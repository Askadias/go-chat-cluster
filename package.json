{
  "name": "go-chat-cluster",
  "version": "1.0.0",
  "description": "Go Chat Cluster",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_ENV=dev && ng serve --aot --disable-host-check --port 4443 --proxy-config proxy.conf.json --watch --ssl 1 --ssl-key 'server/key.pem' --ssl-cert 'server/cert.pem'",
    "start:http": "set NODE_ENV=development && ng serve --aot --disable-host-check --port 8083 --proxy-config proxy.conf.json --watch",
    "build": "ng build",
    "serve": "ng build --aot --watch",
    "pkg:prod": "yarn run pkg --env=prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --prod --aot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/flex-layout": "2.0.0-beta.10-4905443",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/router": "^5.2.5",
    "core-js": "^2.4.1",
    "jwt-decode": "^2.2.0",
    "ngx-cookie-service": "^1.0.10",
    "rxjs": "^5.5.2",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.4.6",
    "@angular/cli": "1.5.4",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/language-service": "^5.2.5",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.101",
    "codelyzer": "^4.1.0",
    "eslint": "^4.19.0",
    "eslint-config-google": "^0.9.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
